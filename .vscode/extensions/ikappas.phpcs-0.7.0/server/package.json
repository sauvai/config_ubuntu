	"publisher": "ikappas",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"homepage": "https://github.com/ikappas/vscode-phpcs/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/ikappas/vscode-phpcs.git"
	},
	"bugs": {
		"url": "https://github.com/ikappas/vscode-phpcs/issues"
