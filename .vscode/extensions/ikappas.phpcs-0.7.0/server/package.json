	"dependencies": {
		"vscode-languageserver": "^2.6.2"
	},
	"devDependencies": {
		"@types/node": "^6.0.52",
		"typescript": "^2.1.4"
	},
	"scripts": {
		"compile": "installServerIntoExtension ../phpcs ./package.json ./tsconfig.json && tsc -p .",
