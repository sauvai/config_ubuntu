{
	"name": "Base16 Dark Default",
	"type": "dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#181818",
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#585858"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#ba8baf"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#ab4642"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.require",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#7cafc2"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"support.class",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#f7ca88"
			}
		},
		{
			"name": "Classes",
			"scope": "meta.class",
			"settings": {
				"foreground": "#f8f8f8"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#7cafc2"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#ba8baf"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#86c1b9"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#a1b56c"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#ab4642"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#7cafc2"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#ba8baf"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#7cafc2"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f7ca88"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ba8baf"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#a1b56c"
			}
		},
		{
			"name": "Link Text",
			"scope": [
				"string.other.link",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#ab4642"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#ab4642"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#dc9656"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#383838",
				"foreground": "#d8d8d8"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#a1b56c"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#ab4642"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ba8baf"
			}
		},
		{
			"name": "Colors",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#86c1b9"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#86c1b9"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#86c1b9"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#a16946"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#ab4642",
				"foreground": "#f8f8f8"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"background": "#dc9656",
				"foreground": "#181818"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#a16946",
				"foreground": "#f8f8f8"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"background": "#585858",
				"foreground": "#f8f8f8"
			}
		}
	],
	"colors": {
		"editor.background": "#181818",
		"editorCursor.foreground": "#d8d8d8",
		"editor.foreground": "#d8d8d8",
		"editorWhitespace.foreground": "#585858",
		"editor.lineHighlightBackground": "#58585855",
		"editor.selectionBackground": "#383838"
	}
}