{
	"name": "Base16 Dark Apathy",
	"type": "dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#031A16",
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#2B685E"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#4C963E"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#3E9688"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.require",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#96883E"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"support.class",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#3E4C96"
			}
		},
		{
			"name": "Classes",
			"scope": "meta.class",
			"settings": {
				"foreground": "#D2E7E4"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#96883E"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#4C963E"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#963E4C"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#883E96"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#3E9688"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#96883E"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#4C963E"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#96883E"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3E4C96"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4C963E"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#883E96"
			}
		},
		{
			"name": "Link Text",
			"scope": [
				"string.other.link",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#3E9688"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#3E9688"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#3E7996"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#184E45",
				"foreground": "#81B5AC"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#883E96"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#3E9688"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#4C963E"
			}
		},
		{
			"name": "Colors",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#963E4C"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#963E4C"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#963E4C"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#3E965B"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#3E9688",
				"foreground": "#D2E7E4"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"background": "#3E7996",
				"foreground": "#031A16"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#3E965B",
				"foreground": "#D2E7E4"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"background": "#2B685E",
				"foreground": "#D2E7E4"
			}
		}
	],
	"colors": {
		"editor.background": "#031A16",
		"editorCursor.foreground": "#81B5AC",
