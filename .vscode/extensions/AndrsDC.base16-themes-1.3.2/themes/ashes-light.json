			"settings": {
				"foreground": "#C79595"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#C7AE95",
				"foreground": "#F3F4F5"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"background": "#C7C795",
				"foreground": "#1C2023"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#C79595",
				"foreground": "#F3F4F5"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"background": "#C7CCD1",
				"foreground": "#1C2023"
			}
		}
