		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#b31e8d"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#00aabb"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#37b349"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#eb008a"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#0e5a94"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#b31e8d"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#0e5a94"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f29333"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b31e8d"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#37b349"
			}
		},
		{
			"name": "Link Text",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#eb008a"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#eb008a"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#e0e0e0",
				"foreground": "#303030"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"background": "#37b349",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"background": "#eb008a",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Colours",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#00aabb"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#00aabb"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#00aabb"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#7a2d00"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#eb008a",
				"background": "#ffffff"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#37b349",
				"background": "#ffffff"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#b31e8d",
				"background": "#ffffff"
			}
		}
	],
	"colors": {
		"editor.background": "#ffffff",
		"editorCursor.foreground": "#303030",
		"editor.foreground": "#303030",
		"editorWhitespace.foreground": "#e0e0e0",
		"editor.lineHighlightBackground": "#d0d0d0",
		"editor.selectionBackground": "#e0e0e0"
	}
}