{
	"name": "Base16 Light Pop",
	"type": "light",
	"tokenColors": [
		{
			"settings": {
				"background": "#ffffff",
				"foreground": "#303030"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#303030"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#303030"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#303030"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#303030"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control"
			],
			"settings": {
				"foreground": "#b31e8d"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#eb008a"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"meta.require"
			],
			"settings": {
				"foreground": "#0e5a94"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"meta.class",
				"support.class",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#f29333"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#0e5a94"
			}
