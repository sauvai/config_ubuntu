#include <stdio.h>
#include "String.h"

int		main(void)
{
  String	*this;

  this = malloc(sizeof(String));
  StringInit(this, "cecile");
  printf("!clear : %s\n", (*this).str);
  (*this).clear(this);
  printf("clear : %s\n", (*this).str);
  StringDestroy(this);
}
