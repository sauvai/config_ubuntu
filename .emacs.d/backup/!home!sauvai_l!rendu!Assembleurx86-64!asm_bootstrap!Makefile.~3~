NAME	=	printf.asm

CC	=	gcc

NA	=	nasm -f elf64

RM	=	rm -rf

SRCS	=	$(NAME).asm \

OBJS	=	$(NAME).o

all:		$(NAME)

$(NAME):	$(OBJS)
		$(NASM) $(SRCS)
		$(CC) -o $(NAME) $(OBJS)

clean:
		$(RM) $(OBJS)

fclean:		clean
		$(RM) $(NAME)

re:		fclean all
