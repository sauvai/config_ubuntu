#include <stdlib.h>
#include <stdio.h>
#include "String.h"

int		main(void)
{
  String	*this;

  this = malloc(sizeof(String));
  StringInit(this, "cristaline");
  printf("%s\n", (*this).c_str(this));
  StringDestroy(this);
  free(this);
  return (0);
}
