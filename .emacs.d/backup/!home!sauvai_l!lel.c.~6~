#include <stdio.h>

int	main(void)
{
  char	num;

  num = '3' | '7'| '4';
  printf("num = %c\n", num);
  if (7 == num)
    printf("num contient 7\n");
  if (6 == num)
    printf("num ne contient pas 6\n");
}
