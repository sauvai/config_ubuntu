#include <unistd.h>
#include <stddef.h>

void	my_putchar(char c)
{
  write(1, &c, 1);
}

void	my_putstr(char *str)
{
  int	i;

  i = 0;
  while (str[i])
    {
      my_putchar(str[i]);
      i += 1;
    }
}

void	*malloc(size_t size)
{
my_putstr("malloc\n");
  return (0);
}
