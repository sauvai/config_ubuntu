NAME	=	asm

CC	=	gcc -g3

NA	=	nasm -f elf64

RM	=	rm -rf

SRCS	=	strlen.asm \
		strchr.asm \

OBJS	=	$(SRCS:.asm=.o)

all:		$(NAME)

$(NAME):
		$(NA) $(SRCS)
		$(CC) -c main.c
		$(CC) -o $(NAME) main.o $(OBJS)

clean:
		$(RM) $(OBJS) main.o

fclean:		clean
		$(RM) $(NAME)

re:		fclean all
