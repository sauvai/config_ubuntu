SERVER		=	server

CLIENT		=	client

SERVER_SRC	=	server.c \

CLIENT_SRC	=	client.c \

SERVER_OBJ	=	$(SERVER_SRC:.c=.o)

CLIENT_OBJ	=	$(CLIENT_SRC:.c=.o)

CFLAGS		=	-W -Wall -Werror -Wextra

CC		=	gcc

RM		=	rm -f

all:		$(SERVER) $(CLIENT)

$(SERVER):	$(SERVER_OBJ)
		$(CC) $(SERVER_OBJ) -o $(SERVER)

$(CLIENT):	$(CLIENT_OBJ)
		$(CC) $(SERVER_OBJ) -o $(SERVER)

clean:
		$(RM) $(SERVER_OBJÃ  $(CLIENT_OBJ)

fclean:		clean
		$(RM) $(SERVER) $(CLIENT)

re:		fclean all
